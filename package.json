    "@testing-library/react": "^14.0.0",
    "@testing-library/react-hooks": "^8.0.1",
    "@types/bootstrap": "^5.2.6",
    "@types/chrome": "^0.0.203",
    "@types/eslint": "^8.4.10",
    "@types/jest": "^29.5.1",
    "@types/node": "^18.11.10",
    "@types/react": "^18.2.14",
